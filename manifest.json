{
  "manifest_version": 3,
  "name": "Bookmark Search",
  "version": "1.0",
  "description": "Search Chrome bookmarks quickly",
  "permissions": [
    "bookmarks",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
